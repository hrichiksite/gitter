<div class="activity-item-inner github-public">
  <i class='octicon octicon-repo activity-item__icon'></i>
  <div class="activity-time" id="time">{{ sentFormatted }}</div>

  <div class="gtrActivityItemText"><span class='mention' data-link-type="mention" data-screen-name="{{payload.sender.login}}">{{payload.sender.login}}</span> made <a href="{{sanitizeHref payload.repository.html_url}}" target="_blank">{{payload.repository.full_name}}</a> public</div>

  <div style="clear: both;"></div>
</div>
